package whileexo;

import java.util.Scanner;

public class exo10 {

	public static void main(String[] args) {
	Scanner clavier = new Scanner(System.in);
	
	
	System.out.println("donner nbr");
	
	int nbr = clavier.nextInt();
	int first =0;
	int compter=nbr % 10;
	
	int last=0;
	
   
	   while(last < compter) {
		   last++;
	   }
		
   if(compter == 0)
   {
	  first = nbr / nbr;
   }
     System.out.println(first +"  " +  last);
     }

}
