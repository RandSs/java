package whileexo;

import java.util.Scanner;

public class exo10 {

	public static void main(String[] args) {
	Scanner clavier = new Scanner(System.in);
	
	
	System.out.println("donner nbr");
	
	int nbr = clavier.nextInt();
	int first=0;
	int i = 0;

	int compter=nbr % 10;
	
	
	int last=0;
	
	 while(first >= 10) {

         first/=10;
		 

	   }
   
	 while(first >=1) {
		 first--;
		 i++;
		 
	 }
	   while(last < compter) {
		   last++;
		  
	   }
	
     System.out.println(i +" first " +  last +" last");
     }

}
